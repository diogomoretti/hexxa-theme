{
  "name": "Hexxa Theme",
  "type": "dark",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#3C3945"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#49DCB1"
      }
    },
    {
      "name": "js template-expression",
      "scope": "punctuation.definition.template-expression.begin.js,punctuation.definition.template-expression.end.js,punctuation.definition.template-expression.begin.ts,punctuation.definition.template-expression.end.ts",
      "settings": {
        "foreground": "#DB5461"
      }
    },
    {
      "name": "TemplateString",
      "scope": [
        "string.template.js",
        "variable.parameter.js"
      ],
      "settings": {
        "foreground": "#49DCB1"
      }
    },
    {
      "name": "TemplateString",
      "scope": "meta.template.expression.js",
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#4D5BFC"
      }
    },
    {
      "name": "Embeded String Begin and End",
      "scope": [
